{
    "baseApiUrl" : "https://localhost:7130",
    "email_validation_pattern" : "^[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,4}$",
    "password_validation_pattern" : "^(?=\\w[a-zA-Z!*@#$%^&+=0-9.]*$)(?=.{8,32}$)(?=.*[A-Z])(?=.*[a-z])(?=.*[0-9])(?=.*[!*@#$%^&+=]).*$",
    "error_codes" : [
        {
            "code" : "invalid_user_id_or_password",
            "message" : "Invalid User ID or Password."
        },
        {
            "code" : "account_exists",
            "message" : "An account already exists with that email address."
        },
        {
            "code" : "unexpected_error",
            "message" : "Hmmm...An unexpected error ocurred."
        }
    ]
}